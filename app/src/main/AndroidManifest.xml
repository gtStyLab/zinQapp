<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.krishikishore.zin_q">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/zinqlogo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/zinqlogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CreditsActivity"></activity>
        <activity android:name=".InstructionsActivity" />
        <activity android:name=".chooseCircle6" />
        <activity android:name=".chooseCircle5" />
        <activity android:name=".chooseCircle4" />
        <activity android:name=".chooseCircle3" />
        <activity android:name=".chooseCircle2" />
        <activity android:name=".chooseCircle" />
        <activity android:name=".ResultsActivity" />
        <activity android:name=".ImageCapture" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>